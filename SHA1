        $mb(rc,text='Round Constant')
        $rb(description)
    Frame End
While End

Frame 41:
        $mt(text='END OF SHA-1',color=red)
        $mb(w1,color=red,bold=True)
        $mb(w2,color=red,bold=True)
        $mb(w3,color=red,bold=True)
        $mb(w4,color=red,bold=True)
        $mb(w5,color=red,bold=True)
