# File for visualizing SHA-3/Keccak Sponge function

Frame 1:
    $modifyTitle(text='SHA-3 256-bit Sponge Function Overview', color='red')
    $db(r0,650,710,300,100,text='r')
    $db(c0,50,710,600,100,text='c')
    $db(p0,750,570,400,100,text='input block 0')
    $db(xor1,650,595,50,50,text='XOR')
    $db(f0,90,570,500,100,text='f')
    $db(r1,650,410,300,100,text='r')
    $db(c1,50,410,600,100,text='c')
    $db(p1,750,280,400,100,text='input block 1')
    $db(xor2,650,305,50,50,text='XOR')
    $db(f1,90,280,500,100,text='f')
    $db(r2,650,120,300,100,text='r')
    $db(c2,50,120,600,100,text='c')
    $db(z0,800,80,200,100,text='output hash')

    $da(p0,xor1)
    $da(r0,xor1)
    $da(c0,f0)
    $da(xor1,f0)

    $da(f0,c1)
    $da(f0,r1)

    $da(r1,xor2)
    $da(p1,xor2)
    $da(c1,f1)
    $da(xor2,f1)

    $da(f1,c2)
    $da(f1,r2)

    $da(r2,z0)
Frame End

Frame 2:
