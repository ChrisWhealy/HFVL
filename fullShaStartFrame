Frame 1:
    $modifyTitle(text='SHA 0 ROUND 0 ', color='red')
    $drawBox(w1,100,700,150,100,text='WORD 1 ROUND 0')
    $drawBox(w2,250,700,150,100,text='WORD 2 ROUND 0')
    $drawBox(w3,400,700,150,100,text='WORD 3 ROUND 0')
