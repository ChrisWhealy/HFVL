$drawBox(box2,200,200,100,100,text='box2')
$drawBox(box3,600,300,100,100,text='box3',color='blue')
$drawArrow(box2,box3,bold=True,color=red)
$drawArrow(box3,box1)
$drawBox(box4,700,00,100,100,text='box4')
$drawBox(box5,350,450,100,100,text='box5')
$drawArrow(box4,box5)
$modifyBox(box2,text='this text changed!!!',color='red')
$modifyArrow(box2,box3,bold=False,color='green')
$modifyBox(box2,bold=True)